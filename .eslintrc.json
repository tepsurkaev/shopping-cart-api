{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "error",
        "semi": "off",
        "comma-dangle": "off",
        "space-before-function-paren": "off"
    }
}
